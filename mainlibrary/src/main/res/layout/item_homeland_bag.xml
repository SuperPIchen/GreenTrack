<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="470dp"
    android:layout_height="350dp">

    <ImageView
        android:id="@+id/wuyong1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/dialog_homeland_bag_background"
        android:adjustViewBounds="true"/>

    <FrameLayout
        android:id="@+id/FlItenRecycleViewContainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/wuyong1"
        android:layout_alignTop="@id/wuyong1"
        android:layout_alignStart="@id/wuyong1"
        android:layout_alignEnd="@id/wuyong1"
        android:layout_marginStart="8dp"
        android:layout_marginTop="38dp"
        android:layout_marginBottom="74dp"
        android:layout_marginEnd="92dp"/>

    <ImageView
        android:id="@+id/IvHomelandBagAward"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:src="@drawable/awardbutton_pressed"
        android:layout_alignEnd="@id/wuyong1"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="8dp"
        android:adjustViewBounds="true"/>

    <ImageView
        android:id="@+id/IvHomelandBagGoods"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:src="@drawable/goodsbutton_unpressed"
        android:layout_alignEnd="@id/wuyong1"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="8dp"
        android:adjustViewBounds="true"/>

    <ImageView
        android:id="@+id/IvHomelandBagFood"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:src="@drawable/foodbutton_unpressed"
        android:layout_alignEnd="@id/wuyong1"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="8dp"
        android:adjustViewBounds="true"/>
</RelativeLayout>